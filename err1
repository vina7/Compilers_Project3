/* Test case 1: duplicate declarations */
program err1;
class myTest
{
    
	declarations
		int x, y;
		int x;
	enddeclarations
    method int func(int m, n)
	declarations
	int m;
    int z;
	enddeclarations
	{
	/* nothing */	
	}

	method void main()
	{
     myTest.func(x,y);
     func('sfjdsjfksd');
	/* nothing */	
	}

}	
